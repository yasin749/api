{
  "name": "project",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 12.18.3 < 13",
    "npm": ">=6.14.6"
  },
  "scripts": {
    "start:dev": "nodemon --exec npm run start",
    "start": "node ./src/app.js",
    "serve": "npm run start -- --release",
    "lint": "eslint ./src",
    "fix": "npm run lint --fix",
    "sequelize": "sequelize",
    "model:generate": "npm run sequelize model:generate -- --name UntitledModel --attributes firstAttr:string",
    "migration:generate": "npm run sequelize migration:generate -- --name UntitledMigration",
    "seed:generate": "npm run sequelize seed:generate -- --name UntitledSeed",
    "db:migrate": "npm run sequelize db:migrate",
    "db:migrate:status": "npm run sequelize db:migrate:status",
    "db:migrate:undo": "npm run sequelize db:migrate:undo",
    "db:migrate:undo:all": "npm run sequelize db:migrate:undo:all",
    "db:seed": "npm run sequelize db:seed",
    "db:seed:undo": "npm run sequelize db:seed:undo",
    "db:seed:all": "npm run sequelize db:seed:all",
    "db:seed:undo:all": "npm run sequelize db:seed:undo:all",
    "db:rebuild": "npm run sequelize db:drop  &&  npm run sequelize db:create  &&  npm run sequelize db:migrate  &&  npm run sequelize db:seed:all",
    "prepare": "husky install"
  },
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-status-codes": "^2.1.4",
    "husky": "^5.1.3",
    "morgan": "~1.9.1",
    "pg": "^7.15.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "swagger-ui-dist": "^3.37.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "nodemon": "^2.0.2",
    "sequelize-cli": "^6.2.0"
  }
}
